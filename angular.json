{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "container": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/container",
      "sourceRoot": "projects/container/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/container",
            "index": "projects/container/src/index.html",
            "main": "projects/container/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/container/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/container/src/favicon.ico",
              "projects/container/src/assets",
              "assets"
            ],
            "styles": [
              "projects/container/src/styles/main.scss",
              "styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/container/src/app",
                "projects/container/src/app/shared",
                "projects/container/src/app/ui",
                "styles",
                "styles/typography"
              ]
            },
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              },
              {
                "type": "bundle",
                "name": "main",
                "maximumWarning": "1mb",
                "maximumError": "2mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "10kb",
                "maximumError": "20kb"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "aot": true,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.dev.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "container:build:production"
            },
            "development": {
              "browserTarget": "container:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "container:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/container/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/container/src/favicon.ico",
              "projects/container/src/assets"
            ],
            "styles": [
              "projects/container/src/styles/main.scss",
              "styles/main.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "configurator": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/configurator",
      "sourceRoot": "projects/configurator/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/configurator",
            "index": "projects/configurator/src/index.html",
            "main": "projects/configurator/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/configurator/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/configurator/src/favicon.ico",
              "projects/configurator/src/assets",
              "assets"
            ],
            "styles": [
              "projects/configurator/src/styles/main.scss",
              "styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/container/src/app",
                "projects/container/src/app/shared",
                "projects/container/src/app/ui",
                "styles",
                "styles/typography"
              ]
            },
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              },
              {
                "type": "bundle",
                "name": "main",
                "maximumWarning": "1mb",
                "maximumError": "2mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "10kb",
                "maximumError": "20kb"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "aot": true,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.dev.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "configurator:build:production"
            },
            "development": {
              "browserTarget": "configurator:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "configurator:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/configurator/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/configurator/src/favicon.ico",
              "projects/configurator/src/assets"
            ],
            "styles": [
              "projects/configurator/src/styles.main.scss",
              "styles/main.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "grid": {
      "projectType": "library",
      "root": "projects/grid",
      "sourceRoot": "projects/grid/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/grid/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/grid/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/grid/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/grid/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "projects/ui",
      "sourceRoot": "projects/ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ui/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
